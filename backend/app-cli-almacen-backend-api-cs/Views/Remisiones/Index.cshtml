                @Html.DisplayNameFor(model => model.DtFecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntIdAgenteDestino)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntIdAgenteOrigen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntIdEstadoRemision)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
