            <td>
                @Html.DisplayFor(modelItem => item.IntIdAgenteDestino)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntIdAgenteOrigen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntIdEstadoRemision)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IntIdRemision">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IntIdRemision">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IntIdRemision">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
