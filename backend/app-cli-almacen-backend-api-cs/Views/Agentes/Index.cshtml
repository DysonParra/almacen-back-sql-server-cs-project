@model IEnumerable<Project.Models.Agentes>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IntIdEntidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntIdAlmacen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrObservaciones)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StrUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DtFecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntIdSociedad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IntIdTipoAgente)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IntIdEntidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntIdAlmacen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrObservaciones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StrUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DtFecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntIdSociedad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IntIdTipoAgente)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IntIdAgente">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IntIdAgente">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IntIdAgente">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
